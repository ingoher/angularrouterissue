{
  "name": "abx",
  "description": "An Ionic project",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "e2e": "ng e2e",
    "ngcc": "ngcc",
    "cypress-test": "npx cypress open --config-file cypress-test.config.ts",
    "cypress:component": "npx cypress open --component --browser chrome --config-file cypress-local.config.ts",
    "cypress:e2e": "npx cypress open --e2e --browser chrome",
    "cypress:e2e-all": "npx cypress run --spec \"cypress/e2e/**/*.feature\" --browser chrome",
    "mockBackend": "npm run start --prefix mock_server",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/{de,en}.json --sort --format namespaced-json --format-indentation '  '",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "checkFilenames": "bash tools/checkFilenames.sh",
    "checkFormat": "prettier --check --config .prettierrc \"src/**/*.{js,ts,json,html,scss,css}\"",
    "checkTranslations": "node tools/checkTranslations.js src/assets/i18n/",
    "checkBuildSuccess": "bash tools/checkBuildSuccess.sh",
    "format": "prettier --write --config .prettierrc \"src/**/*.{js,ts,json,html,scss,css}\"",
    "stylelint": "npx stylelint \"./**/*.scss\" --formatter verbose",
    "stylelint-fix": "npx stylelint \"./**/*.scss\" --fix --formatter verbose",
    "lint": "ng lint && npm run stylelint",
    "lint:fix": "ng lint --fix && npx stylelint \"./**/*.scss\" --fix --formatter verbose",
    "audit": "npm run checkFilenames && npm run lint && npm run checkFormat && npm run checkTranslations && npm run checkBuildSuccess",
    "audit:fix": "npm run checkFilenames && npm run lint:fix && npm run format && npm run checkTranslations && npm run checkBuildSuccess"
  },
  "private": true,
